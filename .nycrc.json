{
  "check-coverage": false,
  "per-file": true,
  "lines": 100,
  "statements": 100,
  "functions": 100,
  "branches": 100,
  "reporter": ["lcov", "text", "text-summary"],
  "require": ["@babel/register"],
  "exclude": [
    "**/mockData/**",
    "coverage/**",
    "test/**",
    "test{,-*}.js",
    "**/*.test.js",
    "**/__tests__/**",
    "**/node_modules/**",
    "**/__snapshots__/**",
    ".nyc_output/**",
    ".nyc_output-tmp/**"
  ],
  "cache": true,
  "all": false,
  "temp-directory": "./.nyc_output-tmp",
  "report-dir": "./.nyc_output"
}
